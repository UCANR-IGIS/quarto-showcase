        "import geopandas as gpd\n",
        "UC_ANR = gpd.read_file(\"UC_ANR.gpkg\")\n",
        "gdf = UC_ANR\n",
        "import folium\n",
        "\n",
        "m = folium.Map(\n",
        "    location=[36.7783, -119.4179],\n",
        "    zoom_start=6,\n",
        "    width='100%',\n",
        "    height='100%'   # Fill the iframe entirely\n",
        ")\n",
        "# Shape and color mapping\n",
        "shape_map = {\n",
        "    'UCCE': {'sides': 30, 'color': 'purple'},           # Circle (approximated with many sides)\n",
        "    'UC Campus': {'sides': 5, 'color': 'red'},       # Pentagon\n",
        "    'REC': {'sides': 3, 'color': 'green'},           # Triangle\n",
        "}\n",
        "\n",
        "# Add markers\n",
        "for idx, row in gdf.iterrows():\n",
        "    symbol = row['CATEGORY']\n",
        "    \n",
        "    if symbol == 'UCOP':\n",
        "        # Star icon\n",
        "        folium.Marker(\n",
        "            location=[row['lat'], row['lon']],\n",
        "            popup=row['Location'],\n",
        "            icon=folium.Icon(color='yellow', icon='star')\n",
        "        ).add_to(m)\n",
        "    else:\n",
        "        folium.RegularPolygonMarker(\n",
        "            location=[row['lat'], row['lon']],\n",
        "            number_of_sides=shape_map[symbol]['sides'],\n",
        "            radius=5,\n",
        "            color=shape_map[symbol]['color'],\n",
        "            fill=True,\n",
        "            fill_color=shape_map[symbol]['color'],\n",
        "            popup=row['Location']\n",
        "        ).add_to(m)\n",
        "\n",
        "m.save(\"map_for_slide.html\")\n",
        "display(HTML('''\n",
        "<div style=\"width:700px; height:450px; margin:auto; overflow:hidden;\">\n",
        "    <iframe src=\"map_for_slide.html\" width=\"100%\" height=\"100%\" style=\"border:none;\"></iframe>\n",
        "</div>\n",
        "'''))"
      ],
      "id": "84adb5e9",
      "execution_count": null,
      "outputs": []
