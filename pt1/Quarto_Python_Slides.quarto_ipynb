        "import os\n",
        "from IPython.display import display, HTML\n",
        "\n",
        "# Folder with images\n",
        "img_folder = r\"image_assets\\staff_circles\"\n",
        "img_files = [os.path.join(img_folder, f) for f in sorted(os.listdir(img_folder)) if f.endswith(\".png\")]\n",
        "\n",
        "# Labels for each image\n",
        "labels = [\n",
        "    \"Maggi Kelly\",\n",
        "    \"Andy Lyons\",\n",
        "    \"Sean Hogan\",\n",
        "    \"Shane Feirer\",\n",
        "    \"Robert Johnson\",\n",
        "    \"Priyanka Vyas\",\n",
        "    \"Genoa Starrs\"\n",
        "]\n",
        "\n",
        "# Generate HTML for a staff row with larger images\n",
        "html = '<div style=\"display: flex; justify-content: center; align-items: flex-start; gap: 15px;\">'  # more gap for bigger images\n",
        "for img, label in zip(img_files, labels):\n",
        "    # Split first and last names to force line break\n",
        "    label_html = '<br>'.join(label.split())\n",
        "    html += f'''\n",
        "    <div style=\"\n",
        "        text-align: center; \n",
        "        font-size: 24px;       /* slightly larger font */\n",
        "        font-weight: 500;      /* moderately bold */\n",
        "        line-height: 1.2em;    /* spacing between lines */\n",
        "        color: #005fae;\n",
        "    \">\n",
        "        <img src=\"{img}\" width=\"130px\" style=\"border-radius: 50%;\"><br>  <!-- larger image -->\n",
        "        {label_html}\n",
        "    </div>\n",
        "    '''\n",
        "html += '</div>'\n",
        "\n",
        "display(HTML(html))"
      ],
